<style>
    .stat-card {
        border: none;
        border-radius: 8px;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    }

    .card-body {
        position: relative;
        padding: 1.5rem;
    }

    .stat-icon {
        font-size: 1.5rem;
        color: #198754;
        /* Bootstrap green */
    }

    .stat-heading {
        font-size: 0.9rem;
        color: #6c757d;
        margin-bottom: 0.25rem;
    }

    .stat-value {
        font-size: 1.6rem;
        font-weight: bold;
    }

    .view-more {
        background-color: #0B2B55;
        /* Dark blue */
        color: white;
        text-decoration: none;
        position: absolute;
        bottom: 0;
        right: 0;
        padding: 0.5rem 1rem;
        border-top-left-radius: 8px;
    }

    .view-more:hover {
        background-color: #083366;
        /* Slightly darker on hover */
    }

    .card-custom {
        border: none;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        border-radius: 10px;
    }

    .status-badge {
        border-radius: 5px;
        padding: 2px 8px;
        font-size: 0.85rem;
    }

    .view-link {
        font-size: 0.9rem;
        text-decoration: none;
        color: #6c757d;
    }

    .view-link:hover {
        color: #000;
    }
</style>

